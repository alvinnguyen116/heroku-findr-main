{
"plugins": [
  ["@babel/plugin-proposal-class-properties"],
  [
    "babel-plugin-styled-components",
    {
      "ssr": true,
      "displayName": true,
      "fileName": true,
      "minify": true,
      "transpileTemplateLiterals": true,
      "pure": true
    }
  ]
],
"presets": [
  [
    "@babel/preset-env",
    {
      "useBuiltIns": "entry",
      "corejs": 3
    }
  ],
  "@babel/preset-react"
  ]
}